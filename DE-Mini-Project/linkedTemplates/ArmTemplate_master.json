{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DE-Mini-Project"
		},
		"LS_Mongodb_Atlas_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Mongodb_Atlas'"
		},
		"LS_Sqldb_Files_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Sqldb_Files'"
		},
		"LS_blob_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob_storage'"
		},
		"LS_Mongodb_Atlas_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"LS_Sqldb_Files_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbs-training.database.windows.net"
		},
		"LS_Sqldb_Files_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_training"
		},
		"LS_Sqldb_Files_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sujithaa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DE-Mini-Project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Mongodb_Atlas_connectionString": {
						"value": "[parameters('LS_Mongodb_Atlas_connectionString')]"
					},
					"LS_Sqldb_Files_password": {
						"value": "[parameters('LS_Sqldb_Files_password')]"
					},
					"LS_blob_storage_connectionString": {
						"value": "[parameters('LS_blob_storage_connectionString')]"
					},
					"LS_Mongodb_Atlas_properties_typeProperties_database": {
						"value": "[parameters('LS_Mongodb_Atlas_properties_typeProperties_database')]"
					},
					"LS_Sqldb_Files_properties_typeProperties_server": {
						"value": "[parameters('LS_Sqldb_Files_properties_typeProperties_server')]"
					},
					"LS_Sqldb_Files_properties_typeProperties_database": {
						"value": "[parameters('LS_Sqldb_Files_properties_typeProperties_database')]"
					},
					"LS_Sqldb_Files_properties_typeProperties_userName": {
						"value": "[parameters('LS_Sqldb_Files_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}